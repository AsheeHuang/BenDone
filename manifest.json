 {
   "name": "Synology Dinner Notification",
   "version": "1.0",
   "description": "Dinner",
   "permissions": ["storage",
				   "declarativeContent",
				   "activeTab",
				   "tabs",
				   "alarms",
				   "notifications",
   				   "<all_urls>"],
   "background": {
		"scripts": ["background.js"],
		"persistent": false
   },
   "page_action": {
	   "default_popup": "popup.html"
   },
   "manifest_version": 2
 }
