 {
   "name": "Synology Dinner Notification",
   "version": "1.0",
   "description": "Dinner",
   "permissions": ["storage",
				   "declarativeContent",
				   "activeTab",
				   "tabs",
				   "alarms",
				   "notifications",
   				   "<all_urls>"],
   "background": {
		"scripts": ["background.js"],
		"persistent": false
   },
   "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "24": "icon24.png",
        "32": "icon32.png"
      }
   },
   "manifest_version": 2
 }
